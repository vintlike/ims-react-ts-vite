{
  "name": "ims-react-ts-vite",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "build:beta": "tsc && vite build --mode beta",
    "build:legacy": "tsc && vite build --mode legacy ",
    "build:release": "tsc && vite build --mode release",
    "build:test": "tsc && vite build --mode test",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "commitlint": "commitlint --from=master",
    "dev": "vite",
    "format": "prettier --cache --write .",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx,.vue --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --quiet --fix --ext .js,.jsx,.ts,.tsx,.vue --no-error-on-unmatched-pattern",
    "lint:style": "stylelint src/**/*.less --fix --custom-syntax postcss-less",
    "prepare": "husky install",
    "prettier": "prettier --write .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@ant-design/icons": "^5.1.4",
    "ahooks": "^3.7.8",
    "antd": "^5.6.3",
    "axios": "^1.4.0",
    "classnames": "^2.3.2",
    "echarts": "^5.4.2",
    "echarts-for-react": "^3.0.2",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.1",
    "react-router": "^6.14.1",
    "react-router-dom": "^6.14.1",
    "redux": "^4.2.1",
    "redux-thunk": "^2.4.2",
    "styled-components": "^6.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.3.3",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "@vitejs/plugin-legacy": "^4.0.5",
    "@vitejs/plugin-react": "^4.0.1",
    "eslint": "^8.44.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "husky": "^8.0.3",
    "less": "^4.1.3",
    "less-loader": "^11.1.3",
    "lint-staged": "^13.2.3",
    "postcss-less": "^6.0.0",
    "prettier": "^2.8.8",
    "prettier-plugin-organize-imports": "^3.2.2",
    "prettier-plugin-packagejson": "^2.4.3",
    "rimraf": "^5.0.1",
    "rollup-plugin-external-globals": "^0.8.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "stylelint": "^15.9.0",
    "stylelint-config-css-modules": "^4.2.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended": "^12.0.0",
    "stylelint-order": "^6.0.3",
    "typescript": "^5.1.6",
    "vite": "^4.3.9",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-svgr": "^3.2.0"
  }
}
